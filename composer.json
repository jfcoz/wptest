{
  "name": "jfcoz/mywp",
  "description": "my website",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    }
  ],
  "config"      : {
    "vendor-dir": "wp-content/vendor"
  },
  "require": {
    "php": ">=5.4",
    "johnpbloch/wordpress": "6.0.0",
    "wp-cli/wp-cli-bundle": "2.6.0",
    "wpackagist-plugin/advanced-custom-fields": "5.12.2",
    "wpackagist-plugin/flush-opcache": "4.1.3",
    "wpackagist-plugin/posts-to-posts": "1.7",
    "wpackagist-plugin/wordpress-seo":"19.2",
    "wpackagist-plugin/wp-nested-pages": "3.1.21",
    "wpackagist-plugin/acf-to-wp-api": "1.4.0",
    "wpackagist-plugin/custom-post-type-ui": "1.12.1",
    "wpackagist-plugin/simple-custom-post-order": "2.5.6",
    "wpackagist-plugin/wordpress-importer": "0.7.0",
    "wpackagist-plugin/wp-rest-api-v2-menus": "0.10.0",
    "wpackagist-plugin/jwt-authentication-for-wp-rest-api": "1.2.6",
    "wpackagist-plugin/wp-all-import": "3.6.7",
    "wpackagist-plugin/wp-maximum-upload-file-size": "1.0.9",
    "wpackagist-plugin/contact-form-7": "5.6",
    "wpackagist-plugin/flamingo": "2.3",
    "wpackagist-plugin/redirection": "5.2.3",
    "wpackagist-plugin/simple-cache": "2.0.0",
    "wpackagist-theme/generatepress":"3.1.3",
    "humanmade/s3-uploads":"3.0.4"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "cp wp/index.php ./index.php",
      "sed -i \"s/\\/wp-blog-header/\\/wp\\/wp-blog-header/g\" index.php"
    ]
  }
}
